<launch>
    <node pkg="urg_node" type="urg_node" name="urg_node">
    <param name="frame_id" value="front_hokuyo_link"/>
    <rosparam>
      publish_intensity: false
      angle_min: -2.3
      angle_max: 2.3
      cluster: 1
      skip: 0
      ip_port: 10940
      serial_port: /dev/sensors/laser/base
      frame_id: laser
      calibrate_time: true
      time_offset: 0.0
      publish_multiecho: false
    </rosparam>
    </node>
</launch>
